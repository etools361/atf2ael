defun u_mix_while_x_for_2()
{
    decl out = 0;
    decl o = 0;
    o = 0;
    while (o < 2)
    {
        decl sum = 0;
        decl i0 = 0;
        decl i1 = 0;
        for (i0 = 0; i0 < 2; i0++)
        {
            for (i1 = 0; i1 < 2; i1++)
            {
                sum = sum + 1;
            }
        }
        o = o + 1;
    }
    return out;
}
