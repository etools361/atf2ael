defun mix_if_x_while_2()
{
    decl out=0;
    if(1)
    {
        decl sum=0;
        decl w0=0;
        decl w1=0;
        w0 = 0;
        while (w0 < 2)
        {
            w1 = 0;
            while (w1 < 2)
            {
                sum = sum + 1;    w1 = w1 + 1;
            }    w0 = w0 + 1;
        }    out = out + 1;
    }
    else
    {
        out = out + 2;
    }
    return out;
}
