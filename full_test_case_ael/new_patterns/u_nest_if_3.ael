defun u_nest_if_3()
{
    decl sum = 0;
    if (sum == 0)
    {
        if (sum == 1)
        {
            if (sum == 2)
            {
                sum = sum + 1;
            }
            else
            {
                sum = sum + 2;
            }
        }
        else
        {
            sum = sum + 2;
        }
    }
    else
    {
        sum = sum + 2;
    }
    return sum;
}
