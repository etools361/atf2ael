// Two switches: dense and sparse cases
decl x = 2;
decl y = 0;

switch (x)
{
    case 0:
        y = 10;
        break;
    case 1:
        y = 20;
        break;
    case 2:
        y = 30;
        break;
    default:
        y = -1;
        break;
}

x = 100;
switch (x)
{
    case 0:
        y = 1;
        break;
    case 100:
        y = 2;
        break;
    case 1000:
        y = 3;
        break;
    default:
        y = 4;
        break;
}

